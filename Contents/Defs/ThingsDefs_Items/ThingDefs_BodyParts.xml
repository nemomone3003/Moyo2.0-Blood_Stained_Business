<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- 모요 꼬리보철물 -->
	<HediffDef ParentName="AddedBodyPartBase">
		<defName>Moyo2_TailProsthesis</defName>
		<label>moyo tail prosthesis</label>
		<labelNoun>a tail prosthesis</labelNoun>
		<description>A simple tail prosthesis for moyos. it doesnt repond fast but better than nothing.</description>
		<descriptionHyperlinks>
			<ThingDef>Moyo2_TailProsthesis</ThingDef>
		</descriptionHyperlinks>
		<spawnThingOnRemoved>Moyo2_TailProsthesis</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>0.85</partEfficiency>
		</addedPartProps>
	</HediffDef>
	<ThingDef ParentName="BodyPartProstheticMakeableBase">
		<defName>Moyo2_TailProsthesis</defName>
		<label>moyo tail prosthesis</label>
		<description>A simple tail prosthesis for moyos. it doesnt repond fast but better than nothing.</description>
		<techLevel>Industrial</techLevel>
		<costList>
			<Moyo2_Cloth_Depthweave>20</Moyo2_Cloth_Depthweave>
			<Steel>10</Steel>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<statBases>
			<Mass>8</Mass>
		</statBases>
		<techHediffsTags>
			<li>Simple</li>
		</techHediffsTags>
	</ThingDef>
	<RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
		<defName>Install_Moyo2_TailProsthesis</defName>
		<label>install moyo tail prosthesis</label>
		<description>Install tail prosthesis.</description>
		<descriptionHyperlinks>
			<ThingDef>Moyo2_TailProsthesis</ThingDef>
			<HediffDef>Moyo2_TailProsthesis</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing tail prosthesis.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Moyo2_TailProsthesis</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Moyo2_TailProsthesis</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Moyo2_Tail</li>
		</appliedOnFixedBodyParts>
		<addsHediff>Moyo2_TailProsthesis</addsHediff>
	</RecipeDef>
	<!-- 모요 생체공학 꼬리 -->
	<HediffDef ParentName="AddedBodyPartBase">
		<defName>Moyo2_TailBionic</defName>
		<label>moyo bionic tail</label>
		<labelNoun>a tail prosthesis</labelNoun>
		<description>artificial tail made with advanced technology. This tail is indistinguishable by looks and its powerful servo combined with neural interface makes it superior compared to natural ones.</description>
		<descriptionHyperlinks>
			<ThingDef>Moyo2_TailBionic</ThingDef>
		</descriptionHyperlinks>
		<spawnThingOnRemoved>Moyo2_TailBionic</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.25</partEfficiency>
		</addedPartProps>
	</HediffDef>
	<ThingDef ParentName="BodyPartBionicBase">
		<defName>Moyo2_TailBionic</defName>
		<label>moyo bionic tail</label>
		<description>artificial tail made with advanced technology. This tail is indistinguishable by looks and its powerful servo combined with neural interface makes it superior compared to natural ones.</description>
		<descriptionHyperlinks>
			<RecipeDef>Install_Moyo2_TailBionic</RecipeDef>
		</descriptionHyperlinks>
		<costList>
			<Moyo2_Cloth_Depthweave>40</Moyo2_Cloth_Depthweave>
			<Plasteel>5</Plasteel>
			<ComponentSpacer>4</ComponentSpacer>
		</costList>
		<statBases>
			<Mass>8</Mass>
		</statBases>
	</ThingDef>
	<RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
		<defName>Install_Moyo2_TailBionic</defName>
		<label>install moyo bionic tail</label>
		<description>Install a bionic tail.</description>
		<descriptionHyperlinks>
			<ThingDef>Moyo2_TailBionic</ThingDef>
			<HediffDef>Moyo2_TailBionic</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing a bionic tail.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Moyo2_TailBionic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Moyo2_TailBionic</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Moyo2_Tail</li>
		</appliedOnFixedBodyParts>
		<addsHediff>Moyo2_TailBionic</addsHediff>
	</RecipeDef>
</Defs>
