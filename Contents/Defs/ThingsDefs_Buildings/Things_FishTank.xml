<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- 물고기 양식장 -->
	<ThingDef ParentName="BenchBase">
		<defName>Moyo_FishTank</defName>
		<label>abysstech aquaculture tank</label>
		<description>A workbench for producing abysstech items. Equipped with various advanced tools, it can handle precise production like components to complex production of making abysteel.</description>
		<thingClass>Moyo2.ThingClass_FishTank</thingClass>
		<tickerType>Rare</tickerType>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/Production/Crematorium</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3,2)</drawSize>
			<shadowData>
				<volume>(1.95,0.5,1.3)</volume>
			</shadowData>
			<damageData>
				<rect>(0.3,0.1,2.4,1.8)</rect>
			</damageData>
		</graphicData>
		<costList>
			<Steel>250</Steel>
			<ComponentIndustrial>5</ComponentIndustrial>
		</costList>
		<altitudeLayer>Building</altitudeLayer>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<WorkToBuild>4500</WorkToBuild>
			<MaxHitPoints>300</MaxHitPoints>
			<Cleanliness>-20</Cleanliness>
			<Beauty>-10</Beauty>
			<Flammability>0.5</Flammability>
		</statBases>
		<size>(3,2)</size>
		<designationCategory>Production</designationCategory>
		<fillPercent>1</fillPercent>
		<passability>Impassable</passability>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<interactionCellIcon>DiningChair</interactionCellIcon>
		<surfaceType>Item</surfaceType>
		<canOverlapZones>false</canOverlapZones>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<constructionSkillPrerequisite>7</constructionSkillPrerequisite>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>600</basePowerConsumption>
				<shortCircuitInRain>false</shortCircuitInRain>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>6</glowRadius>
				<glowColor>(80,80,217,0)</glowColor>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<forceShowRoomStats>true</forceShowRoomStats>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
			<paintable>false</paintable>
		</building>
		<placeWorkers>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
		<researchPrerequisites>
			<li>Electricity</li>
		</researchPrerequisites>
		<modExtensions>
			<li Class="Moyo2.ModExtension_FishTank">
				<FishDefs>
					<li>MoyoFish_Tuna</li>
					<li>MoyoFish_DemiKraken</li>
				</FishDefs>
			</li>
		</modExtensions>
	</ThingDef>
</Defs>