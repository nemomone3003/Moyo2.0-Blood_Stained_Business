<Defs>
	<!-- 어비스틸 제작 -->
	<RecipeDef>
		<defName>Moyo2_Make_AbyssteelA</defName>
		<label>refine abyssteel</label>
		<description>Craft abyssteel, a commonly used alloy in the lighthouse.</description>
		<jobString>Crafting abyssteel.</jobString>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Smelt</soundWorking>
		<workAmount>1000</workAmount>
		<recipeUsers>
			<li>Moyo2_ArcFurnace</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Uranium</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasteel</li>
				<li>Uranium</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Moyo2_Abyssteel>20</Moyo2_Abyssteel>
		</products>
		<skillRequirements>
			<Crafting>6</Crafting>
		</skillRequirements>
	</RecipeDef>
