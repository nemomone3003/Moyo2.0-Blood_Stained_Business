				<filter>
					<thingDefs>
						<li>Chemfuel</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Chemfuel</li>
			</thingDefs>
			<categories>
				<li>Leathers</li>
			</categories>
