<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationConditional">
		<xpath>Defs/ThingDef[defName="Apparel_Vacsuit"]/apparel/useWornGraphicMask</xpath>
