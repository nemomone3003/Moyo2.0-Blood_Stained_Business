			<li>
				<key>Apparel_Vacsuit</key>
				<value>Apparel/ApparelReplacer/MoyoVac</value>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Moyo"]/alienRace/graphicPaths/apparel/individualPaths</xpath>
		<value>
			<li>
				<key>Apparel_VacsuitHelmet</key>
				<value>Apparel/ApparelReplacer/MoyoVacHead</value>
			</li>
		</value>
	</Operation>	
</Patch>