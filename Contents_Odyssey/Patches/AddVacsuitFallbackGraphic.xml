			<xpath>Defs/ThingDef[defName="Apparel_Vacsuit"]/apparel</xpath>
			<value>
				<useWornGraphicMask>true</useWornGraphicMask>
			</value>
		</nomatch>
	</Operation>
	<Operation Class="PatchOperationConditional">
		<xpath>Defs/ThingDef[defName="Apparel_VacsuitHelmet"]/apparel/useWornGraphicMask</xpath>
