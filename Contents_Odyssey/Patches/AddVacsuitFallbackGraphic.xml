			<xpath>Defs/ThingDef[defName="Apparel_VacsuitHelmet"]/apparel</xpath>
			<value>
				<useWornGraphicMask>true</useWornGraphicMask>
			</value>
		</nomatch>
	</Operation>	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Moyo"]/alienRace/graphicPaths/apparel/individualPaths</xpath>
