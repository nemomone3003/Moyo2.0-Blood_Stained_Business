	<GeneDef ParentName="GeneSkinColorOverride">
		<defName>Moyo2_Skin_Depthborn</defName>
		<label>depthborn skin pattern</label>
		<labelShortAdj>depthborn pattern</labelShortAdj>
		<description>Moyos with this gene will have depthborn skin pattern.</description>
		<iconPath>Icon/DepthbornIcon1</iconPath>
		<skinColorOverride>(255, 255, 255)</skinColorOverride>
		<randomBrightnessFactor>0</randomBrightnessFactor>
		<displayOrderInCategory>93</displayOrderInCategory>
		<displayCategory>Moyo2_Gene</displayCategory>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>depthborn</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>depthborn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	<!-- 스텟 유전자 -->
	<GeneDef ParentName="GeneMoveSpeedBase">
		<defName>Moyo2_MoveSpeed_Slug</defName>
		<label>sluggish runner</label>
		<labelShortAdj>sluggish</labelShortAdj>
		<description>Carriers of this gene move much more slowly than normal.</description>
		<iconPath>Icon/SlugSpeedIcon</iconPath>
		<displayOrderInCategory>0</displayOrderInCategory>
		<statOffsets>
			<MoveSpeed>-0.4</MoveSpeed>
		</statOffsets>
		<biostatMet>5</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>slow</symbol>
				</li>
				<li>
					<symbol>plod</symbol>
				</li>
				<li>
					<symbol>grado</symbol>
				</li>
				<li>
					<symbol>letho</symbol>
				</li>
				<li>
					<symbol>slug</symbol>
				</li>
				<li>
					<symbol>slack</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>slug</symbol>
				</li>
				<li>
					<symbol>plodder</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	<GeneDef>
		<defName>Moyo2_WorkSpeed_Slow</defName>
		<label>deliberate worker</label>
		<labelShortAdj>deliberate</labelShortAdj>
		<description>Carriers of this gene work more slowly than normal.</description>
		<iconPath>Icon/SlowWorkIcon</iconPath>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>0</displayOrderInCategory>
		<statOffsets>
			<WorkSpeedGlobal>-0.1</WorkSpeedGlobal>
		</statOffsets>
		<biostatMet>2</biostatMet>
	</GeneDef>	
</Defs>