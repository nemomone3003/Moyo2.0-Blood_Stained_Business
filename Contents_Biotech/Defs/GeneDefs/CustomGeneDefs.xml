	<!-- 스텟 유전자 -->
	<GeneDef ParentName="GeneMoveSpeedBase">
		<defName>Moyo2_MoveSpeed_Slug</defName>
		<label>sluggish runner</label>
		<labelShortAdj>sluggish</labelShortAdj>
		<description>Carriers of this gene move much more slowly than normal.</description>
		<iconPath>Icon/SlugSpeedIcon</iconPath>
		<displayOrderInCategory>0</displayOrderInCategory>
		<statOffsets>
			<MoveSpeed>-0.4</MoveSpeed>
		</statOffsets>
		<biostatMet>5</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>slug</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>slug</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	<GeneDef>
		<defName>Moyo2_WorkSpeed_Slow</defName>
		<label>deliberate worker</label>
		<labelShortAdj>deliberate</labelShortAdj>
		<description>Carriers of this gene work more slowly than normal.</description>
		<iconPath>Icon/SlowWorkIcon</iconPath>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>0</displayOrderInCategory>
		<statOffsets>
			<WorkSpeedGlobal>-0.1</WorkSpeedGlobal>
		</statOffsets>
		<biostatMet>2</biostatMet>
		<biostatCpx>2</biostatCpx>
	</GeneDef>
	<GeneDef>
		<defName>Moyo2_WorkSpeed_Fast</defName>
		<label>efficient worker</label>
		<labelShortAdj>efficient</labelShortAdj>
		<description>Carriers of this gene work faster than normal.</description>
		<iconPath>Icon/FastWorkIcon</iconPath>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>0</displayOrderInCategory>
		<statOffsets>
			<WorkSpeedGlobal>0.1</WorkSpeedGlobal>
		</statOffsets>
		<biostatMet>-3</biostatMet>
		<biostatCpx>2</biostatCpx>
	</GeneDef>
	<GeneDef>
		<defName>Moyo2_DeepblueMetabolism</defName>
		<label>hyper deepblue metabolism</label>
		<labelShortAdj>deepblue metabolism</labelShortAdj>
		<description>Carriers of this gene uses all deepblue produced in their body for metabolism purposes.</description>
		<iconPath>Icon/FastWorkIcon</iconPath>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>0</displayOrderInCategory>
		<statFactors>
			<DeepblueStatSpeed>0</DeepblueStatSpeed>
			<DeepblueStat>0</DeepblueStat>
		</statFactors>
		<biostatMet>5</biostatMet>
		<biostatCpx>7</biostatCpx>
	</GeneDef>
