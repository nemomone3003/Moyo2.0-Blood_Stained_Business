<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- HPF.HPFWorkGiverDef is inheritance of WorkGiverDef with some additional arguments -->
	<WorkGiverDef Name="Moyo2_HPFWorkGiverBase" Abstract="True">
		<giverClass>Moyo2_HPF.WorkGiver_GatherPawnResources</giverClass>
		<priorityInType>100</priorityInType>
	</WorkGiverDef>

	<WorkGiverDef ParentName="Moyo2_HPFWorkGiverBase">
		<defName>Moyo2_ExtractDeepBlue_Work</defName>
		<label>draw blood</label>
		<workType>Doctor</workType>
		<verb>draw blood</verb>
		<gerund>drawing blood</gerund>
		<priorityInType>80</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
		<modExtensions>
			<li Class="Moyo2_HPF.ModExtension">
				<harvestJobDef>Moyo2_ExtractDeepBlue_Job</harvestJobDef>
			</li>
		</modExtensions>
	</WorkGiverDef>
</Defs>