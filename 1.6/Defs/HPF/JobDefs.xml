<Defs>
	<!-- HPF.HPFJobDef is inheritance of JobDef with some additional arguments -->
	<JobDef Name="Moyo2_HPFJobBase" Abstract="True">
		<driverClass>Moyo2_HPF.JobDriver_GatherPawnResources</driverClass>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
		<!-- isSelf defaults to false -->
	</JobDef>

