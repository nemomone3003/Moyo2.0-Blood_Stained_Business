	<JobDef ParentName="Moyo2_HPFJobBase">
		<defName>Moyo2_ExtractDeepBlue_Job</defName>
		<reportString>extracting Deep blue from TargetA.</reportString>
		<modExtensions>
			<li Class="Moyo2_HPF.ModExtension">
				<totalWork>50</totalWork>
				<activeSkill>Medicine</activeSkill>
				<activeStat>MedicalTendSpeed</activeStat>
			</li>
		</modExtensions>
	</JobDef>

